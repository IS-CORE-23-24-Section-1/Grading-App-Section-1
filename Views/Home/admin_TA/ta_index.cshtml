@{
}

@*@model -----*@

<h1>TA's</h1>

<table>
    <thead>
        <tr>
            <td>TA Name</td>
            <td>Assigned Class</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var x in Model)
        {
        <tr>
            <td>@x.first_name @x.last_name</td>
            <td>@x.class_assignment</td>

            <td><a class="btn btn-info" asp-controller="Home" asp-action="Edit" asp-route-id="@x.netid">Edit</a></td>
            <td><a class="btn btn-danger" asp-controller="Home" asp-action="Delete" asp-route-id="@x.netid">Delete</a></td>
        </tr>
        }
    </tbody>
</table>
<button type="sumbit">Add TA</button>

